{
  "env": {
    "NEXT_PUBLIC_PERSONA_API_URL": "https://api.personapass.xyz/api",
    "NEXT_PUBLIC_PERSONACHAIN_RPC": "https://rpc.personapass.xyz:26657",
    "NEXT_PUBLIC_PERSONACHAIN_API_URL": "https://rpc.personapass.xyz:1317"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}